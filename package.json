{
  "name": "@formkit/auto-animate",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "description": "Automatically animate your html.",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "repository": "https://github.com/formkit/auto-animate.git",
  "author": "Justin Schroeder <justin@formkit.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cd docs && vite --config=./vite.config.ts",
    "build": "npx rollup -c rollup.config.js && FRAMEWORK=vue npx rollup -c rollup.config.js",
    "build-docs": "cd docs && vite-ssg build --config=./docs/vite.config.ts"
  },
  "exports": {
    "./vue": {
      "import": "./dist/vue.mjs"
    },
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-ssg": "^0.20.0",
    "vue": "^3.2.33"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@vueuse/head": "^0.7.6",
    "vue-github-button": "^3.0.3",
    "vue-router": "4"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "react": "^16.8.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
